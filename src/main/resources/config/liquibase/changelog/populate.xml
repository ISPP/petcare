<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

<!--Checksums are applied to changesets tags and they are not recommended to be changed,
that is why we create a custom changeset to populate a data base with examples. We shouldnt
 write a load tag in a generated changeset tag, please create new ones. To follow a method,
 id should be populateX where X is the name of the entity -->
<changeSet author="valentin" id="populateAdministrator">

    <loadData encoding="UTF-8"
              file="config/liquibase/administrator.csv"
              separator=";"
              tableName="administrator"/>

</changeSet>

    <changeSet author="valentin" id="populateCustomer">

        <loadData encoding="UTF-8"
                  file="config/liquibase/customer.csv"
                  separator=";"
                  tableName="customer"/>

    </changeSet>
    <changeSet author="valentin" id="populatePetOwner">

        <loadData encoding="UTF-8"
                  file="config/liquibase/pet_owner.csv"
                  separator=";"
                  tableName="pet_owner"/>
    </changeSet>

</databaseChangeLog>
